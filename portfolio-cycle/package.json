{
  "name": "portfolio-cycle",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@webpack-blocks/babel6": "^0.4.1",
    "@webpack-blocks/dev-server2": "^0.4.0",
    "@webpack-blocks/extract-text2": "^0.4.0",
    "@webpack-blocks/postcss": "^0.4.2",
    "@webpack-blocks/sass": "^0.4.0",
    "@webpack-blocks/tslint": "^0.4.0",
    "@webpack-blocks/typescript": "^0.4.0",
    "@webpack-blocks/webpack2": "^0.4.0",
    "autoprefixer": "^6.7.7",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-custom-element-classes": "^0.1.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babili-webpack-plugin": "0.0.11",
    "copy-webpack-plugin": "^4.0.1",
    "cyclejs-test-helpers": "^1.1.0",
    "html-looks-like": "^1.0.2",
    "html-webpack-plugin": "^2.28.0",
    "jsverify": "^0.8.2",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "nyc": "^10.2.0",
    "progress-bar-webpack-plugin": "^1.9.3",
    "snabbdom-pragma": "^1.8.0",
    "snabbdom-to-html": "^3.1.1",
    "typescript": "^2.2.2",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "@cycle/dom": "^17.1.0",
    "@cycle/http": "^13.2.0",
    "@cycle/run": "^3.1.0",
    "@cycle/time": "^0.8.0",
    "cycle-onionify": "^3.1.0",
    "typestyle": "^1.3.2",
    "xstream": "^10.3.0"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack --config webpack.config.js",
    "test": "NODE_ENV=test nyc mocha-webpack --timeout=100000 --colors --webpack-config webpack.config.test.js test/**/*.test.*",
    "build": "NODE_ENV=production webpack --config webpack.config.js"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "reporter": [
      "html",
      "text-summary"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    },
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          },
          "useBuiltIns": true
        }
      ]
    ],
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "Snabbdom.createElement"
        }
      ]
    ]
  }
}